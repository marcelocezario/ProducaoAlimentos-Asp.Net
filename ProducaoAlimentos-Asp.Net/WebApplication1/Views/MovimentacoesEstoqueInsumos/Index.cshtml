@model IEnumerable<WebApplication1.Models.MovimentacaoEstoqueInsumo>

@{
    ViewBag.Title = "Index";
}

<h2>Movimentação Estoque Insumos</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model._LoteInsumo.ID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DataMovimentacao)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Qtde)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ValorMovimentacao)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item._LoteInsumo.ID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DataMovimentacao)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Qtde)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ValorMovimentacao)
        </td>
    </tr>
}

</table>
